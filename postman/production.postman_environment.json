{
  "id": "prod-env",
  "name": "Production",
  "values": [
    {
      "key": "baseUrl",
      "value": "https://mediabit.go.ro",
      "type": "default",
      "enabled": true
    }
  ],
  "examples": {
    "browser_action": {
      "action": "browser",
      "parameters": "\"https://example.com\", 800, 600, 0, 0"
    },
    "cmd_action": {
      "action": "cmd",
      "parameters": "echo hello world"
    },
    "get_email_action": {
      "action": "get_email",
      "parameters": "\"alex@mediabit.ro\", \"INBOX\", 1"
    },
    "get_email_action_alt": {
      "action": "get_email",
      "parameters": "\"hello@mediabit.ro\", \"INBOX\", 1"
    },
    "get_email_action_gmail": {
      "action": "get_email",
      "parameters": "\"alexandruiga@gmail.com\", \"INBOX\", 1"
    },
    "_photoshop_note": {
      "note": "Every Photoshop process must start with 'create' command and end with 'save' command to be complete"
    },
    "photoshop_create": {
      "action": "photoshop",
      "parameters": "{\"command\": \"create\", \"width\": 800, \"height\": 600, \"name\": \"test\"}"
    },
    "photoshop_add_text": {
      "action": "photoshop",
      "parameters": "{\"command\": \"addText\", \"text\": \"Hello World\", \"x\": 400, \"y\": 300, \"size\": 48, \"color\": [0,0,0]}"
    },
    "photoshop_add_image": {
      "action": "photoshop",
      "parameters": "{\"command\": \"addImage\", \"imagePath\": \"test/custom-image.jpg\", \"x\": 0, \"y\": 0}"
    },
    "photoshop_add_image_resized": {
      "action": "photoshop",
      "parameters": "{\"command\": \"addImage\", \"imagePath\": \"test/custom-image.jpg\", \"x\": 0, \"y\": 0, \"width\": 400, \"height\": 300}"
    },
    "photoshop_save": {
      "action": "photoshop", 
      "parameters": "{\"command\": \"save\", \"path\": \"c:/hub/core/tasker/output/test.psd\", \"format\": \"psd\"}"
    }
  }
}
